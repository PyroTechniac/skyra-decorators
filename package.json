{
  "name": "klasa-decorators",
  "version": "0.0.2",
  "description": "TypeScript decorators for Klasa",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "https://github.com/kyranet/klasa-decorators",
  "author": "kyranet <kyradiscord@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "prepublishOnly": "yarn build",
    "build:browser": "webpack",
    "lint": "eslint --ext ts src --fix",
    "test:lint": "eslint --ext ts src",
    "test": "ava",
    "build": "tsc -p .",
    "watch": "tsc -p . -w"
  },
  "devDependencies": {
    "@types/ws": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^2.5.0",
    "@typescript-eslint/parser": "^2.5.0",
    "ava": "^2.4.0",
    "discord.js": "github:discordjs/discord.js#master",
    "eslint": "^6.5.1",
    "eslint-config-bamboo": "^3.0.0",
    "klasa": "github:dirigeants/klasa#settings",
    "source-map-support": "^0.5.13",
    "ts-node": "^8.4.1",
    "typescript": "^3.6.4"
  },
  "peerDependencies": {
    "discord.js": "github:discordjs/discord.js#master",
    "klasa": "github:dirigeants/klasa#settings"
  },
  "files": [
    "dist/index.d.ts",
    "dist/index.js",
    "dist/index.js.map"
  ],
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "files": [
      "src/test/**/*.ts",
      "!src/test/lib"
    ],
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ]
  },
  "engines": {
    "node": ">=10.1.0"
  }
}
