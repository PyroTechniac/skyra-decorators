{
  "name": "klasa-decorators",
  "version": "0.0.2",
  "description": "TypeScript decorators for Klasa",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "https://github.com/kyranet/klasa-decorators",
  "author": "kyranet <kyradiscord@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "prepublishOnly": "yarn build",
    "build:browser": "webpack",
    "lint": "eslint --ext ts src test --fix",
    "test:lint": "eslint --ext ts src test",
    "test": "ava",
    "build": "tsc -b src",
    "watch": "tsc -wb src"
  },
  "devDependencies": {
    "@types/ws": "^6.0.4",
    "@typescript-eslint/eslint-plugin": "^2.10.0",
    "@typescript-eslint/parser": "^2.10.0",
    "ava": "^2.4.0",
    "discord.js": "github:discordjs/discord.js#master",
    "eslint": "^6.7.2",
    "eslint-config-bamboo": "^3.0.0",
    "klasa": "github:dirigeants/klasa#settings",
    "source-map-support": "^0.5.16",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.3"
  },
  "peerDependencies": {
    "discord.js": "github:discordjs/discord.js#master",
    "klasa": "github:dirigeants/klasa#settings"
  },
  "files": [
    "dist",
    "!dist/tsconfig.tsbuildinfo"
  ],
  "ava": {
    "compileEnhancements": false,
    "extensions": [
      "ts"
    ],
    "files": [
      "test/**/*.test.ts"
    ],
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ]
  },
  "engines": {
    "node": ">=10.1.0"
  }
}
